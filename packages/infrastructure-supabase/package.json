{
  "name": "infrastructure-supabase",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "supabase:gen-types": "supabase gen types typescript --linked --schema public > src/types/models.ts",
    "supabase:reset": "supabase db reset --linked",
    "snaplet:gen-cli": "snaplet generate",
    "snaplet:seed": "bunx tsx seed.mts > supabase/seed.sql",
    "gen": "bun supabase:gen-types && bun snaplet:gen-cli",
    "db:reset": "bun snaplet:seed && bun supabase:reset",
    "lint": "eslint src/",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@snaplet/seed": "^0.77.3",
    "@types/node": "^20.10.6",
    "snaplet": "^0.77.3",
    "supabase": "^1.127.4"
  },
  "peerDependencies": {
    "typescript": "latest"
  }
}